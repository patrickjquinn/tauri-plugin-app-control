{
  "name": "tauri-plugin-app-control-api",
  "version": "0.1.0",
  "author": "You",
  "description": "Tauri plugin for controlling app lifecycle",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run build",
    "check": "tsc --noEmit --project guest-js/tsconfig.json && cargo check"
  },
  "keywords": ["tauri", "plugin", "android", "app-control", "lifecycle"],
  "dependencies": {
    "@tauri-apps/api": "^2.5.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.2",
    "typescript": "^5.8.3"
  }
}
